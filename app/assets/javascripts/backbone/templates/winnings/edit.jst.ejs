<h1>Edit winning</h1>

<form id="edit-winning" name="winning">
  <div class="field">
    <label for="date_start"> date_start:</label>
    <input type="text" name="date_start" id="date_start" value="<%= date_start %>" >
  </div>

  <div class="field">
    <label for="date_end"> date_end:</label>
    <input type="text" name="date_end" id="date_end" value="<%= date_end %>" >
  </div>

  <div class="field">
    <label for="x"> x:</label>
    <input type="text" name="x" id="x" value="<%= x %>" >
  </div>

  <div class="field">
    <label for="measure"> measure:</label>
    <input type="text" name="measure" id="measure" value="<%= measure %>" >
  </div>

  <div class="field">
    <label for="type"> type:</label>
    <input type="text" name="type" id="type" value="<%= type %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Winnings" />
  </div>

</form>

